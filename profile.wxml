<view class='container'>
  <!-- user -->
  <view class='user'>
    <image src='{{user.url}}' class='userinfo-avatar' bacground-size='cover'></image>
    <view>{{user.name}}</view>
    <view class="integral">毛积分：{{user.points}}</view>
  </view>
  <!--setting  -->
  <view class='navigator_bar'>
    <view class="weui-cells weui-cells_after-title">
      <view wx:for='{{navigatorbarlist}}' class="weui_sub">
        <navigatorbar navigator_url='{{item.navigatorurl}}' message='{{item.message}}' imgurl='{{item.imgurl}}' />
      </view>
    </view>
  </view>
</view>