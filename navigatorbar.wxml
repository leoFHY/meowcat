<!--components/navigatorbar/navigator.wxml-->
<navigator url="{{navigator_url}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
  <view class="weui-cell__hd">
    <image src="{{imgurl}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
  </view>
  <view class="weui-cell__bd" style='padding-left:30rpx;'>{{message}}</view>
  <view class="weui-cell__ft weui-cell__ft_in-access">{{navigator_msg}}</view>
</navigator>